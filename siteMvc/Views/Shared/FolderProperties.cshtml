@model FolderViewModel
@using Quantumart.QP8.WebMvc.ViewModels.Library
@using Quantumart.QP8.Resources

@using (Html.BeginTabForm(Model))
{
	using (Html.BeginFieldGroup(FolderStrings.BasicParameters))
	{ 
		@Html.TextBoxFieldFor(src => src.Data.Name);
	}
