<?xml version="1.0"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <Fragment>
    <UI>
      <Dialog Id="UpdateQP8Dlg" Width="370" Height="270" Title="!(loc.UpdateQP8DlgTitle)">
        <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="!(loc.WixUINext)"></Control>
        <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="!(loc.WixUICancel)">
          <Publish Event="SpawnDialog" Value="CancelDlg">1</Publish>
        </Control>
        <Control Id="Back" Type="PushButton" X="168" Y="243" Width="56" Height="17" Text="!(loc.WixUIBack)"></Control>
        <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="!(loc.VerifyReadyDlgBannerBitmap)" />
        <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="2" />
        <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="2" />
        <Control Id="InstallTitle" Type="Text" X="15" Y="15" Width="300" Height="15" Transparent="yes" NoPrefix="yes" Hidden="yes" Text="!(loc.UpdateQP8DlgInstallTitle)">
          <Condition Action="show">NOT Installed</Condition>
        </Control>
        <Control Id="label1" Type="Text" Height="31" Width="350" X="9" Y="56" Text="!(loc.UpdateQP8DlgMessage)" />
        <Control Id="cbUpgradeDatabases" Type="CheckBox" Height="18" Width="157" X="9" Y="89" Text="!(loc.UpdateQP8DlgUpgradeDatabases)" CheckBoxValue="0" Property="UPGRADE_DB"/>
      </Dialog>
    </UI>
  </Fragment>
</Wix>